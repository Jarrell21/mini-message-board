<%- include("header") %>
<h1><%= title %></h1>
<% if (messages && messages.length) { %>
<ul>
  <% messages.forEach((element) => { %>
  <li>
    <span><%= element.text %></span>
    <form
      style="display: inline"
      action="/messages/<%= element.id %>"
      method="get"
    >
      <input type="submit" value="Open" />
    </form>
  </li>
  <% }); %>
</ul>
<% } %>
